*** Test Cases ***
{%- for testcase in testcases %}
{{ testcase.name }}
    {%- if testcase.doc %}
    [Documentation]    {%- for line in testcase.doc|splitlines %}
    ...  {{ line }}
    {%- endfor %}
    {%- endif %}
    {%- for step in testcase.steps %}
    {%- if step.table_var %}
    FOR   {{ rf_join(step.table_columns) }}   IN   {{ step.table_var }}
        {{ call_keyword(step) }}
    END
    {%- else %}
    {% if step.return_values %}{{ rf_join(step.return_values) }}    {% endif %}{{ call_keyword(step) }}
    {%- endif %}
    {%- endfor %}
{%- endfor %}
